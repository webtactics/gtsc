{% extends "layouts/grid-default.njk" %}


{% block content %}
<div class="container">
	<main class="content">

<div class="grid-7x4">

<div class="contentleft">
    <div class="titlebox fade-in appear">
       <h1 class="heading-text">{{ title }}</h1>
    </div>
	{{ content | safe }}


</div>


<aside class="sidebar">
    <div class="sidebarbox">
       <h2 class="heading-text">2024-25 Season Club Night Results</h2>
	{% set postslist = collections['2024-25-Results-GTSC'] -%} 
	{% include "components/postslist-results-sidebar.njk" %}
    </div>
</aside>

</div>

<div class="eightypercentborder"></div>
<div class="resultswrapper">
      <h2 class="heading-text">2024-25 Season Events</h2>
	{% set postslist = collections['2024-25-Season-Events'] | limit(6) %} 
	{% include "components/postslist-events.njk" %}
</div>
<div class="linkwrapper"><a class="resultspdf" href="/events/" title="2024-25 Season Events" alt="2024-25 Season Events">For all Event Listings &gt;</a></div>



	</main>
</div>

{% endblock %}


